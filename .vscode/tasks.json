{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/Application/Adapter/Binance/src/MarketIntelligency.Application.Adapter.Binance.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/MarketIntelligency.Application.Adapter.MercadoBitcoin/MarketIntelligency.Application.Adapter.MercadoBitcoin.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/MarketIntelligency.Application.Adapter.MercadoBitcoin/MarketIntelligency.Application.Adapter.MercadoBitcoin.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "appId": "binance-adapter",
            "appPort": 5000,
            "appProtocol": "grpc",
            "grpcPort": 50001,
            "httpPort":3500,
            "metricsPort": 9090,
            "label": "daprd-debug-binance-adapter",
            "type": "daprd"
        },
        {
            "appId": "binance-adapter",
            "label": "daprd-down-binance-adapter",
            "type": "daprd-down"
        },
        {
            "appId": "data-event-manager",
            "appPort": 5001,
            "appProtocol": "grpc",
            "grpcPort": 50002,
            "httpPort":3501,
            "metricsPort": 9091,
            "label": "daprd-debug-data-event-manager",
            "type": "daprd"
        },
        {
            "appId": "data-event-manager",
            "label": "daprd-down-data-event-manager",
            "type": "daprd-down"
        },
        {
            "appId": "strategies",
            "appPort": 5002,
            "appProtocol": "grpc",
            "grpcPort": 50003,
            "httpPort":3502,
            "metricsPort": 9092,
            "label": "daprd-debug-strategies",
            "type": "daprd"
        },
        {
