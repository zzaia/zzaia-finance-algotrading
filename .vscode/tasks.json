{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/Application/Adapter/Binance/src/MarketIntelligency.Application.Adapter.Binance.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/MarketIntelligency.Application.Adapter.MercadoBitcoin/MarketIntelligency.Application.Adapter.MercadoBitcoin.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/MarketIntelligency.Application.Adapter.MercadoBitcoin/MarketIntelligency.Application.Adapter.MercadoBitcoin.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "appId": "app",
            "appPort": 5000,
            "label": "daprd-debug",
            "type": "daprd"
        },
        {
            "appId": "app",
            "label": "daprd-down",
            "type": "daprd-down"
        }
    ],
    "compounds": [
      {
        "name": "MarketIntelligency",
        "configurations": ["ASP.NET Binance Adapter[Dapr]","ASP.NET Data Event Manager[Dapr]","ASP.NET Strategies[Dapr]"]
      }
    ]
}